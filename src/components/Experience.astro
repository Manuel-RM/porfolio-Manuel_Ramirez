---
import ExperienceItem from "./ExperienceItem.astro";

const EXPERIENCIE = [
	{
		date: "2023",
		title: "Practicante | H. Congreso del Estado de Michoacan de Ocampo.",
		description:
			"Encargado de desarrollar una plataforma web para el departamento de Recursos Humanos, para la optimizacion en la captura de datos y documentos de sus trabajadores.",
		link: "",
	},
];
---

<ol class="relative border-s border-gray-200 ml-3">
	{
		EXPERIENCIE.map((experiencie) => (
			<li class="mb-10 ms-4">
				<ExperienceItem {...experiencie} />
			</li>
		))
	}
</ol>
